programa {
  funcao inicio() {

    real base
    real altura
    real resultado

    escreva("Qual o valor da base? ")
    leia(base)
    escreva("Qual o valor da altura? ")
    leia(altura)
    resultado = base*altura/2
    escreva("O valor da área do triângulo é: " + resultado + "m²")
    
  }
}
